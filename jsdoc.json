{
	"source": {
	  "include": ["docs/src"],
	  "includePattern": ".js$"
	},
	"tags": {
		"allowUnknownTags": ["category"]
	},
	"plugins": [
		"plugins/markdown",
		"node_modules/better-docs/category"
	],
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": true,
		"better-docs": {
			"name": "Runalysis: Developer documentation",
			"navigation": [
				{
					"label": "App",
					"href": "https://kulisak12.github.io/Runalysis/"
				},
				{
					"label": "Github",
					"href": "https://github.com/kulisak12/Runalysis"
				},
				{
					"label": "User documentation",
					"href": "https://kulisak12.github.io/Runalysis/userdoc/index.html"
				}
			]
		}
	},
	"opts": {
		"recurse": true,
		"destination": "./docs/devdoc/",
		"template": "node_modules/better-docs",
		"readme": "docs/devdoc.md"
	}
}